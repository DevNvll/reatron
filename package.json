{
  "name": "reatron",
  "productName": "Reatron",
  "version": "1.0.0",
  "description": "Minimal Electron, React and Redux app boilerplate",
  "main": "main.js",
  "scripts": {
    "test": "npm test",
    "start": "electron .",
    "dev": "webpack --config ./webpack.config.babel.js --watch",
    "pack": "electron-packager ./ Reatron --platform=win32 --arch=ia32 --out=./releases --prune --ignore=\"(/app|.gitignore|webpack.config.babel.js|.babelrc|/releases).*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/razr9/reatron.git"
  },
  "keywords": [
    "react",
    "electron"
  ],
  "author": "Henrick Mello",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.3.3",
    "electron-reload": "^1.0.2",
    "react": "^0.14.8",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.11.6",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.10.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  }
}
